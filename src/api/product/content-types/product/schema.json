{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "text"
    },
    "image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "price": {
      "type": "biginteger"
    },
    "unit": {
      "type": "string"
    },
    "uniqueName": {
      "type": "text"
    },
    "users_permissions_role": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "stock": {
      "type": "biginteger"
    },
    "basePrice": {
      "type": "biginteger"
    },
    "shop": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::shop.shop"
    },
    "grocery": {
      "type": "component",
      "repeatable": false,
      "component": "product.product"
    },
    "productId": {
      "type": "string"
    }
  }
}
